// Raylib Core Types
API_TYPE(PUBLIC, Vector2,
  float x;
  float y;
)

API_TYPE(PUBLIC, Vector3,
  float x;
  float y;
  float z;
)

API_TYPE(PUBLIC, Vector4,
  float x;
  float y;
  float z;
  float w;
)

API_TYPE(PUBLIC, Color,
  unsigned char r;
  unsigned char g;
  unsigned char b;
  unsigned char a;
)

API_TYPE(PUBLIC, Rectangle,
  float x;
  float y;
  float width;
  float height;
)

API_TYPE(PUBLIC, Camera3D,
  Vector3 position;
  Vector3 target;
  Vector3 up;
  float fovy;
  int projection;
)

API_TYPE(PUBLIC, Texture2D,
  unsigned int id;
  int width;
  int height;
  int mipmaps;
  int format;
)
